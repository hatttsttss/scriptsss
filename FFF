-- Wait for game to load
if not game:IsLoaded() then
    game.Loaded:Wait()
end

-- Wait for player
if not player then
    game:GetService("Players").PlayerAdded:Wait()
    player = game:GetService("Players").LocalPlayer
end

-- Wait for PlayerGui
if not player:FindFirstChild("PlayerGui") then
    player:WaitForChild("PlayerGui")
end

-- Global variables with menu protection
local lib = nil
local mainMenu = nil
local menuLoaded = false
local menuLoadInProgress = false
local menuLoadAttempts = 0
local MAX_MENU_ATTEMPTS = 2

-- Cleanup any existing menus first
pcall(function()
    local playerGui = player:FindFirstChild("PlayerGui")
    if playerGui then
        for _, gui in pairs(playerGui:GetChildren()) do
            if gui:IsA("ScreenGui") and (gui.Name:find("XZR") or gui.Name:find("Main") or gui.Name:find("Library")) then
                gui:Destroy()
            end
        end
    end
end)

